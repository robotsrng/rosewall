<div class="container">

  <form id="calculator-selector">

    <input type="radio" name="calculator-selector" value="square-calculator"> Rectangle<br>
    <input type="radio" name="calculator-selector" value="cone-calculator"> Cone<br>
    <input type="radio" name="calculator-selector" value="cylinder-calculator"> Cylinder<br>
  </form>
  <div id="calculator-container" class="calculator-container">
    <p>Please select a calculator type.</p>
  </div>

  <ol class="breadcrumb">
    <li class="breadcrumb-item">
      <a href="/">Home</a>
    </li>
    <li class="breadcrumb-item active">Calculator</li>
  </ol>
</div>

<div id="myModal" class="modal" tabindex="-1" role="dialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header ">
        <h5 class="modal-title">Yardage Calculator</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body js-calculator-total">
        <p>Modal body text goes here.</p>
      </div>
      <div class="modal-footer ">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>